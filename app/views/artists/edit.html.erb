<h3>Artist Form</h3>

<%= @artist.name%><br>
<%= @artist.bio%>

<%= form_for @artist do |f|%>
<%= f.label :name%>
<%= f.text_field :name%>
<%= f.label :bio%>
<%= f.text_field :bio%>
<%= f.submit%>

<%end%>






