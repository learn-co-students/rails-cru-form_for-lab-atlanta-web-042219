<h3>Song Form</h3>

<%= @song.name%><br>


<%= form_for @song do |f|%>
<%= f.label :name%>
<%= f.text_field :name%>
<%= f.label :artist_id%>
<%= f.text_field :artist_id%>
<%= f.label :genre_id%>
<%= f.text_field :genre_id%>

<%= f.submit%>

<%end%>